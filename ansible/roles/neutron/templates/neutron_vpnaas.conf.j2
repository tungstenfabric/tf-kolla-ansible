{% if enable_neutron_vpnaas | bool %}
[service_providers]
{% if neutron_plugin_agent == 'vmware_nsxv' %}
service_provider = VPN:vmware:vmware_nsx.services.vpnaas.nsxv.ipsec_driver.NSXvIPsecVpnDriver:default
{% elif neutron_plugin_agent == 'opencontrail-ml2' %}
service_provider = 'L3_ROUTER_NAT:TF:networking_opencontrail.l3.service_provider.TFL3ServiceProvider'
{% else %}
service_provider = VPN:openswan:neutron_vpnaas.services.vpn.service_drivers.ipsec.IPsecVPNDriver:default
{% endif %}

{% endif %}
